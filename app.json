{
  "expo": {
    "name": "Gambit",
    "slug": "gambit",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0A"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.maxwellyoung.socialchaos",
      "buildNumber": "5"
    },
    "android": {
      "package": "com.maxwellyoung.socialchaos",
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#0A0A0A"
      }
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "e6174ea5-eeff-4f99-8854-c98cffaf4d89"
      }
    },
    "assetBundlePatterns": ["**/*", "assets/images/*", "assets/fonts/*"],
    "fonts": [
      "./assets/fonts/Inter-Black.otf",
      "./assets/fonts/Inter-Bold.otf",
      "./assets/fonts/Inter-Medium.otf",
      "./assets/fonts/Inter-Regular.otf",
      "./assets/fonts/Inter-SemiBold.otf"
    ]
  }
}
